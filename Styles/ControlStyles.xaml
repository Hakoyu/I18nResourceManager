    <Setter Property="pu:ProgressBarHelper.CornerRadius" Value="4,4,4,4" />
    <Setter Property="pu:ProgressBarHelper.ShadowColor" Value="#FF2B97FF" />
    <Setter Property="pu:ProgressBarHelper.AnimationEasing" Value="BackIn" />
